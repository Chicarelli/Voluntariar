<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voluntariado">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".AprovedMembers" />
        <activity android:name=".MyProfileEditActivity" />
        <activity android:name=".MyProfileActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".TelaEventoParticipante" />
        <activity android:name=".MeusEventos" />
        <activity android:name=".SolicitacoesPendentes"
            android:parentActivityName=".MeusEventos">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".MeusEventos"/>
        </activity>
        <activity android:name=".ChatActivity" />
        <activity android:name=".PerfilMembros" />
        <activity android:name=".RequestParticipation" />
        <activity android:name=".telaDoEvento" />
        <activity android:name=".CriarEventoFisico" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>